package ClasesYObjetos;

import javax.swing.JOptionPane;

public class Main {

    public static void main (String []args) {
        
        int n1 = Integer.parseInt(JOptionPane.showInputDialog("Digite el primer numero "));
        
        int n2 = Integer.parseInt(JOptionPane.showInputDialog("Digite el segundo numero "));
        
        System.out.println("Los numeros digitados son: "+n1+" y "+n2);
        
        Operacion op = new Operacion();
        
        System.out.println("La suma es: "+op.sumar(n1, n2));
        System.out.println("La resta es: "+op.restar(n1, n2));
        System.out.println("La multiplicación es: "+op.multiplicar(n1, n2));
        System.out.println("La división es: "+op.division(n1, n2));
        
        
    }
}
